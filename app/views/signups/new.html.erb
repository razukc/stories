<div class="contact-main"> 
  <%= image_tag image_path("open.svg"), :alt => "Open", :class=>"icon-menu", :width=>"24px" %>
         <div class="content container">
            <h1>Get the latest updates from Stories</h1>
            <div class="row">
                <div class="col-md-4 col-md-4-offset"></div>
                <div class="col-md-4">
                  <%= form_for(@signup) do |f| %>
                    <!--form accept-charset="UTF-8" action="/signups" class="new_signup" id="new_signup" method="post" -->
                        
                        <div class="form-group">
                          <%= f.label :first_name %>
                          <%= f.text_field :first_name, :class=>"form-control", :id=>"signup_firstname" %>
 </div>
                        <div class="form-group">
                          <%= f.label :email %>
                          <%= f.text_field :email, :class=>"form-control", :id=>"signup_email" %>
                           </div>
                  <%= f.submit "Join", :class=>"btn btn-default" %>
                  <% end %>
  
                </div>
            </div>
            <div class="col-md-4 col-md-4-offset"></div>
        </div>
    </div>